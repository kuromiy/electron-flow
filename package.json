{
	"name": "electron-flow",
	"version": "3.0.0",
	"description": "A simple utility library with helpful functions",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"postinstall": "npm run build",
		"test": "tsx ./test/index.test.ts && tsx ./test/events.test.ts",
		"prepare": "husky",
		"lint": "npx @biomejs/biome check",
		"check": "tsc --noEmit -p tsconfig.test.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kuromiy/electron-flow.git"
	},
	"keywords": [
		"utility",
		"library",
		"typescript",
		"functions"
	],
	"author": "kuromiya",
	"license": "UNLICENSED",
	"type": "module",
	"bugs": {
		"url": "https://github.com/kuromiy/electron-flow/issues"
	},
	"homepage": "https://github.com/kuromiy/electron-flow#readme",
	"devDependencies": {
		"@biomejs/biome": "2.2.2",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@types/node": "^24.3.1",
		"husky": "^9.1.7",
		"tsx": "^4.20.5"
	},
	"dependencies": {
		"typescript": "^5.9.2"
	},
	"browser": false,
	"exports": {
		".": {
			"node": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./result": {
			"import": "./dist/result.js",
			"require": "./dist/result.js",
			"types": "./dist/result.d.ts"
		}
	}
}
